{% for item in array %}
<li> {{ item }}
{% endfor %}

{% for i = 3; --i > 0; %}
<li> i = {{ i }}
{% endfor %}

{% for attr in user %}
<li>{{ loop.i }} User {{ loop.key }} is {{ attr }}
{% endfor %}

{% for attr in deeper.object %}
<li> User has a {{ attr }}
{% endfor %}

{% for attr in deeper.not_found %}
<li> User has a {{ attr }}
{% endfor %}

{% for item in ["a", "b", "c"] %}
<li> {{ item }}
{% endfor %}

